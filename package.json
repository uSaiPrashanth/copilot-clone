{
	"enableProposedApi": true,
	"name": "copilot-clone",
	"displayName": "Captain Stack",
	"description": "Find snippets codes from Stackoverflow",
	"version": "0.0.1",
	"publisher": "hieunc229",
	"icon": "icon.png",
	"repository": "https://github.com/Microsoft/vscode-extension-samples",
	"engines": {
		"vscode": "^1.34.0"
	},
	"license": "MIT",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.copilot-clone-settings",
				"title": "Copilot-clone Settings"
			}
		],
		"menus": {
			"editor/inlineCompletions/actions": [
				{
					"command": "extension.copilot-clone-settings"
				}
			]
		},
		"configuration":{
			"title": "copilotclone",
			"properties": {
				"copilotclone.key_value":{
					"type":"string",
					"default":null,
					"description": "AI21 Studios API KEY"
				},
				"copilotclone.usemodel":{
					"type":"string",
					"enum": ["jumbo","large"],
					"default":"jumbo",
					"description": "AI21 Studios Model to be selected"
				},
				"copilotclone.searchphasestart":{
					"type":"string",
					"default":"#Python"
				},
				"copilotclone.searchphaseend":{
					"type":"string",
					"default":"."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./",
		"download-api": "vscode-dts dev",
		"postdownload-api": "vscode-dts main",
		"postinstall": "npm run download-api"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/node-fetch": "^2.5.10",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"eslint": "^7.21.0",
		"typescript": "^4.2.2",
		"vscode-dts": "^0.3.1"
	},
	"dependencies": {
		"@types/jsdom": "^16.2.12",
		"axios": "^0.21.1",
		"jsdom": "^16.6.0",
		"node-fetch": "^2.6.1"
	}
}
